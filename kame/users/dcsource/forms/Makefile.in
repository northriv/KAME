#>+ 14
dcsourceform.cpp: $(srcdir)/dcsourceform.ui dcsourceform.h dcsourceform.moc
	rm -f dcsourceform.cpp
	echo '#include <klocale.h>' > dcsourceform.cpp
	$(UIC) -tr ${UIC_TR} -i dcsourceform.h $(srcdir)/dcsourceform.ui > dcsourceform.cpp.temp ; ret=$$?; \
	$(PERL) -pe "s,${UIC_TR}( \"\" ),QString::null,g" dcsourceform.cpp.temp | $(PERL) -pe "s,${UIC_TR}( \"\"\, \"\" ),QString::null,g" | $(PERL) -pe "s,image([0-9][0-9]*)_data,img\$$1_dcsourceform,g" >> dcsourceform.cpp ;\
	rm -f dcsourceform.cpp.temp ;\
	if test "$$ret" = 0; then echo '#include "dcsourceform.moc"' >> dcsourceform.cpp; else rm -f dcsourceform.cpp ; exit $$ret ; fi

dcsourceform.h: $(srcdir)/dcsourceform.ui
	$(UIC) -o dcsourceform.h $(srcdir)/dcsourceform.ui

dcsourceform.moc: dcsourceform.h
	$(MOC) dcsourceform.h -o dcsourceform.moc
