prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ftgl
Description: OpenGL frontend to Freetype 2
Version: 2.0.5
Requires: 
Libs: -lGLU -L/usr/X11R6/lib -lGL -L/usr/X11R6/lib -lfreetype -L${libdir} -lftgl
Cflags: -I${includedir} -I/usr/X11R6/include -I/usr/X11R6/include -I/usr/X11R6/include/freetype2
